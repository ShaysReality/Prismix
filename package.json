{
  "name": "prismix-monorepo",
  "private": true,
  "version": "0.2.2",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm -w prismix-core run build && npm -w prismix-cli run build",
    "dev": "npm --ws run dev",
    "lint": "eslint . --ext .ts,.tsx || true",
    "generate": "node packages/cli/dist/index.cjs generate -c examples/blog/prismix.config.json"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "typescript": "^5.5.4"
  }
}
